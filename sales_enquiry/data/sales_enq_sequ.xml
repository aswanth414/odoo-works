<odoo>
    <data noupdate="1">

        <!-- No Update = 1 means sequence will not be realoaded each time module is updated -->

        <record id="seq_sales_enq" model="ir.sequence">
            <field name="name">Enquiry Num</field>
            <field name="code">sales.enquiry.sequence</field>
            <field name="prefix">SE</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
<!--            <field name="company_id" eval="False"/>-->
            <field name="padding">3</field>
        </record>

    </data>
</odoo>